		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="9.0.8" PrivateAssets="all" />
		<PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.3.0" />
	</ItemGroup>

	<ItemGroup>
		<EditorConfigFiles Remove="C:\Users\Dean\source\repos\MyResume\DisplayResume\.editorconfig" />
	</ItemGroup>

	<ItemGroup>
		<None Include="C:\Users\Dean\source\repos\MyResume\DisplayResume\.editorconfig" />
	</ItemGroup>

</Project>
